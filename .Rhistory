library(tidyverse)
library(ggeffects)
mtcars <- mtcars %>%
mutate(cyl = as.factor(cyl),
am = as.factor(am))#,
m <- lm(mpg ~ cyl + vs, data = mtcars)
pred <- ggpredict(m, , terms = c("cyl", "am"), type = "fe") %>%
rename(mpg = predicted,
cyl = x,
am = group)
m <- lm(mpg ~ cyl + am, data = mtcars)
pred <- ggpredict(m, , terms = c("cyl", "am"), type = "fe") %>%
rename(mpg = predicted,
cyl = x,
am = group)
ggplot() +
geom_point(aes(x = cyl, y = mpg), mtcars, color = "grey") +
geom_point(aes(x = cyl, y = mpg), pred) +
geom_errorbar(aes(x = cyl, y = mpg, ymin = conf.low, ymax = conf.high), pred) +
facet_grid(~ am)
